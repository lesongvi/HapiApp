<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ScheduleProject" parent="ThemeOverlay.MaterialComponents.Dark.ActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00B489</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/MMGray</item>
        <!-- Customize your theme here. -->
        <item name="android:background">@color/teal_700</item>


    </style>

    <style name="Theme.ScheduleProject.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="TabLayout" parent="CardView.Light"/>

    <style name="Theme.ScheduleProject.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ScheduleProject.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>