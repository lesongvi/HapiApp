<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.scheduleproject">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hutech University"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ScheduleProject">
        <activity android:name=".Login"></activity>
        <activity
            android:name=".RLuyen"
            android:theme="@style/Theme.ScheduleProject.NoActionBar"></activity>
        <activity
            android:name=".LichThi"
            android:theme="@style/Theme.ScheduleProject.NoActionBar"></activity>
        <activity
            android:name=".TKB"
            android:theme="@style/Theme.ScheduleProject.NoActionBar" />
        <activity
            android:name=".XemDiem"
            android:theme="@style/Theme.ScheduleProject.NoActionBar" />
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.ScheduleProject.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>